[Nemo Action]
# View windows overview
Name=Window overview (Scale)
Comment=Open overview to add, remove, or organize windows
Exec=sh -c 'dbus-send --dest=org.Cinnamon --print-reply /org/Cinnamon org.Cinnamon.ShowOverview >/dev/null'
Selection=None
Extensions=any;
Conditions=desktop;dbus org.Cinnamon;
Icon-Name=view-app-grid-symbolic
